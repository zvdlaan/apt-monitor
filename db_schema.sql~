
--CREATE DATABASE DB_VandyApt;

DROP TABLE IF EXISTS AmbientRoomTemp;

CREATE TABLE AmbientRoomTemp
(
	Id INT NOT NULL AUTO_INCREMENT,
	MeasuredTime TIMESTAMP,
	Temp DECIMAL,
	PRIMARY KEY (Id)
);

